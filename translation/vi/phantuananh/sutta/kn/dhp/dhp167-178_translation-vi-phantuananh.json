{
  "dhp167:0.1": "Tiểu Bộ Kinh",
  "dhp167:0.2": "Kinh Pháp Cú",
  "dhp167:0.3": "Phẩm Thế Gian",
  "dhp167:0.4": "Sự tích vị Tỳ-kheo trẻ",
  "dhp167:1": "Chớ theo pháp hạ liệt.",
  "dhp167:2": "Chớ sống mặc, buông lung;",
  "dhp167:3": "Chớ tin theo tà kiến,",
  "dhp167:4": "Chớ tăng trưởng tục trần.",
  "dhp168:0": "Sự tích vua Suddhodana  (Tịnh Phạn)",
  "dhp168:1": "Nỗ lực, chớ phóng dật!",
  "dhp168:2": "Hãy sống theo chánh hạnh;",
  "dhp168:3": "Người chánh hạnh hưởng lạc,",
  "dhp168:4": "Cả đời này, đời sau."
}