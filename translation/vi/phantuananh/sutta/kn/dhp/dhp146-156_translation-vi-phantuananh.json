{
  "dhp146:0.1": "Tiểu Bộ Kinh",
  "dhp146:0.2": "Kinh Pháp Cú",
  "dhp146:0.3": "Phẩm Già",
  "dhp146:0.4": "Sự tích những người bạn của bà Visākhā (Thiện Chi)",
  "dhp146:1": "Cười gì, hân hoan gì,",
  "dhp146:2": "Khi đời mãi bị thiêu?",
  "dhp146:3": "Bị tối tăm bao trùm,",
  "dhp146:4": "Sao không tìm ngọn đèn?",
  "dhp147:0": "Sự tích kỹ nữ Sirimā",
  "dhp147:1": "Hãy xem bong bóng đẹp,",
  "dhp147:2": "Chỗ chất chứa vết thương,",
  "dhp147:3": "Bệnh hoạn nhiều suy tư,",
  "dhp147:4": "Thật không gì trường cửu.",
  "dhp148:0": "Sự tích Trưởng lão ni Uttarā",
  "dhp148:1": "Sắc này bị suy già,",
  "dhp148:2": "Ổ tật bệnh, mỏng manh,",
  "dhp148:3": "Nhóm bất tịnh đổ vỡ,",
  "dhp148:4": "Chết chấm dứt mạng sống.",
  "dhp149:0": "Sự tích nhóm Tỳ-kheo tăng thượng mạn",
  "dhp149:1": "Như trái bầu mùa thu,",
  "dhp149:2": "Bị vất bỏ quăng đi,",
  "dhp149:3": "Nhóm xương trắng bồ câu,",
  "dhp149:4": "Thấy chúng còn vui gì?",
  "dhp150:0": "Sự tích Trưởng lão ni Rūpanandā (Sắc Khánh Hỷ)",
  "dhp150:1": "Thành này làm bằng xương,",
  "dhp150:2": "Quét tô bằng thịt máu,",
  "dhp150:3": "Ở đây già và chết,",
  "dhp150:4": "Mạn, lừa đảo chất chứa.",
  "dhp151:0": "Sự tích hoàng hậu\nMallikā (Mạt-lỵ) băng hà",
  "dhp151:1": "Xe vua đẹp cũng già,",
  "dhp151:2": "Thân này rồi sẽ già.",
  "dhp151:3": "Pháp bậc thiện, không già.",
  "dhp151:4": "Như vậy bậc chí thiện\nNói lên cho bậc thiện.",
  "dhp152:0": "Sự tích Trưởng lão Loḷudāyi"
}