{
  "thig5.12:0.1": "Verses of the Elder Bhikkhunīs",
  "thig5.12:0.2": "The Chapter of the Fives",
  "thig5.12:0.3": "Verses of the Elder Candā",
  "thig5.12:1.1": "\"Before I was miserable,",
  "thig5.12:1.2": "a widow without children,",
  "thig5.12:1.3": "without friends or relatives,",
  "thig5.12:1.4": "I did not acquire cloth or food.",
  "thig5.12:2.1": "Having taken bowl and walking stick,",
  "thig5.12:2.2": "I went for alms from family to family.",
  "thig5.12:2.3": "Consumed by heat and cold,",
  "thig5.12:2.4": "I practiced for seven years."
}