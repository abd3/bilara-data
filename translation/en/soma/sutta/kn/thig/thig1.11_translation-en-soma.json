{
  "thig1.11:0.1": "Verses of the Elder Bhikkhunīs",
  "thig1.11:0.2": "The Chapter of the Ones",
  "thig1.11:0.3": "Verses of the Elder Muttā",
  "thig1.11:1.1": "\"I am well released and free,",
  "thig1.11:1.2": "freed from the three deceits,",
  "thig1.11:1.3": "of the mortar and the pestle,",
  "thig1.11:1.4": "and the deceitful husband.",
  "thig1.11:1.5": "I am free from birth and death,"
}