{
  "thig2.10:0.1": "Verses of the Elder Bhikkhunīs",
  "thig2.10:0.2": "The Chapter of the Twos",
  "thig2.10:0.3": "Verses of the Elder Samā",
  "thig2.10:1.1": "\"Four times, five times,",
  "thig2.10:1.2": "I went out from the dwelling place.",
  "thig2.10:1.3": "I had not attained peace of mind,",
  "thig2.10:1.4": "had no power over the mind.",
  "thig2.10:1.5": "It's been the eighth night",
  "thig2.10:1.6": "since craving was uprooted.",
  "thig2.10:2.1": "Amidst so much suffering,"
}