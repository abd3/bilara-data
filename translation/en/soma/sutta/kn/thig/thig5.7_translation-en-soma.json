{
  "thig5.7:0.1": "Verses of the Elder Bhikkhunīs",
  "thig5.7:0.2": "The Chapter of the Fives",
  "thig5.7:0.3": "Verses of the Elder Sakulā",
  "thig5.7:1.1": "\"I was living at home,",
  "thig5.7:1.2": "when I heard the Dhamma from a monastic.",
  "thig5.7:1.3": "I saw the stainless Dhamma,",
  "thig5.7:1.4": "Nirvana, the everlasting state.",
  "thig5.7:2.1": "I left behind daughter and son,",
  "thig5.7:2.2": "wealth and grain.",
  "thig5.7:2.3": "I cut the hair off my head,",
  "thig5.7:2.4": "went forth into homelessness.",
  "thig5.7:3.1": "I am training myself to peace,",
  "thig5.7:3.2": "cultivating the path forward.",
  "thig5.7:3.3": "I eliminated lust and anger,",
  "thig5.7:3.4": "and eighteen??? corruptions.",
  "thig5.7:4.1": "Having become fully ordained as a Bhikkhunī,",
  "thig5.7:4.2": "I recollected my earlier birth."
}