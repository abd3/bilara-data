{
  "snp5.13:0.1": "Anthology of Discourses",
  "snp5.13:0.2": "The Questions of Bhadrāvudha",
  "snp5.13:1.1": "“You are the shelter-leaver, craving-cutter, imperturbable,”",
  "snp5.13:1.2": "said Venerable Bhadrāvudha,",
  "snp5.13:1.3": "“delight-renouncer, flood-crosser, freed one,",
  "snp5.13:1.4": "formulation-renouncer, intelligent one."
}