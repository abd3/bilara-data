{
  "thig5.1:0.1": "Canti delle Anziane Bhikkhunī",
  "thig5.1:0.2": "Capitolo dei Cinque",
  "thig5.1:0.3": "Canto di una certa Ignota Anziana",
  "thig5.1:1.1": "“Per venticinque anni",
  "thig5.1:1.2": "da quando abbandonai il mondo",
  "thig5.1:2.2": "traboccavo di avidità",
  "thig5.1:2.3": "e con le braccia in alto, urlando a squarcia gola,",
  "thig5.1:2.4": "entrai nel monastero.",
  "thig5.1:3.1": "Approcciai una bhikkhunī"
}