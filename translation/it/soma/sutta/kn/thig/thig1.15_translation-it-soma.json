{
  "thig1.15:0.1": "Canti delle Anziane Bhikkhunī",
  "thig1.15:0.2": "Capitolo degli Uno",
  "thig1.15:0.3": "Canto dell'Anziana Uttarā",
  "thig1.15:1.1": "\"Ero controllata con il corpo,",
  "thig1.15:1.2": "la parola e la mente.",
  "thig1.15:1.3": "Dopo aver estirpato il desiderio alla radice,",
  "thig1.15:1.4": "sono rinfrescata e senza sete.\"",
  "thig1.15:2.1": "L'Anziana Uttarā"
}