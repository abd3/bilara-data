{
  "thig5.12:0.1": "Canti delle Anziane Bhikkhunī",
  "thig5.12:0.2": "Capitolo dei Cinque",
  "thig5.12:0.3": "Canto dell’Anziana Candā",
  "thig5.12:1.1": "“Ero indigente,",
  "thig5.12:1.2": "una vedova senza figli,",
  "thig5.12:1.3": "amici o parenti.",
  "thig5.12:1.4": "Cibo e stoffa erano difficili da ottenere,",
  "thig5.12:2.1": "quindi presa ciotola e bastone,",
  "thig5.12:2.3": "Vagabondai per sette anni,",
  "thig5.12:2.4": "consumata dal caldo e dal freddo."
}