{
  "thig1.4:0.1": "Canti delle Anziane Bhikkhunī ",
  "thig1.4:0.2": "Capitolo degli Uno ",
  "thig1.4:0.3": "Canto dell’Anziana Tissā ",
  "thig1.4:1.1": "“Allenati con disciplina, Tissā, ",
  "thig1.4:1.2": "non farti sopraffare da alcun giogo. ",
  "thig1.4:1.3": "Sottratta a ogni giogo, ",
  "thig1.4:1.4": "cammina nel mondo senza influenze.” ",
  "thig1.4:2.1": "L’Anziana Tissā"
}